CREATE TABLE `users` (
 `id` int(5) unsigned NOT NULL AUTO_INCREMENT,
 `username` varchar(12) NOT NULL,
 `psales` char(1) NOT NULL DEFAULT 'N',
 `po` char(1) NOT NULL DEFAULT 'N',
 `cptran` char(1) NOT NULL DEFAULT 'N',
 `outtran` char(1) NOT NULL DEFAULT 'N',
 `gl` char(1) NOT NULL DEFAULT 'N',
 `stkmain gldept` varchar(2) NOT NULL DEFAULT '*',
 `recon` char(1) NOT NULL DEFAULT 'N',
 `startupform` varchar(50) NOT NULL,
 `invoiceform` char(2) NOT NULL DEFAULT '01',
 `TIMESTAMP` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
 `outlet` char(2) NOT NULL DEFAULT '""',
 `cashier` char(1) NOT NULL DEFAULT 'N',
 `ba-junior` char(1) NOT NULL DEFAULT 'N',
 `possales` char(1) NOT NULL DEFAULT 'N',
 `storekeeper` char(1) NOT NULL DEFAULT 'N',
 `shopname` varchar(100) NOT NULL DEFAULT 'shop name',
 `street` varchar(100) NOT NULL DEFAULT 'street',
 `STORENUM` char(2) NOT NULL DEFAULT '',
 `phone` char(15) NOT NULL DEFAULT '',
 `superuser` char(1) NOT NULL DEFAULT 'N',
 `customer` char(1) NOT NULL DEFAULT 'N',
 PRIMARY KEY (`id`),
 UNIQUE KEY `id` (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=9 DEFAULT CHARSET=utf8
